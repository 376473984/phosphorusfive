event:pf.web.widgets.panel
  lambda
    lambda
      add:@/+4/?node
        :@/../*/!/../*/lambda/!/../*/parent/?node
      set:@/+3/?value
        :@/../?value
      if:!
        :@/./+2/element/?node
        lambda
          add:@/././+2/?node
            element:div
      if:!
        :@/./+/*/render-type/?node
        and:@/./+/*/element/(/=p/|/=li/)?node
        lambda
          add:@/././+/?node
            render-type:NoClose
      pf.web.widgets.container