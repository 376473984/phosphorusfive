
/*
 * Returns default [controls] type of page
 */


list-files:/system42/cms/templates/
if:x:/-/*/"/system42/cms/templates/main.hl"
  insert-before:x:/./+2/0/0
    src:"template:/system42/cms/templates/main.hl"
else
  insert-before:x:/./+/0/0
    src:"template:{0}"
      :x:/../*/list-files/0?name
insert-before:x:/../0
  src
    role:root
    controls:@"literal:foo
  element:p
  innerValue:Howdy Controls World!"


