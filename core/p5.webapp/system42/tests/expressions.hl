/*
 * Contains expression unit tests for system
 */

/*
 * Tests that "identity expression" behaves as expected!
 */
identity-expression
  insert-before:x:
    src
      foo:bar
  if:x:/../0?name
    !=:foo
    throw:@"Assert error, expected [foo], got [{0}]!"
      :x:/../0?name

