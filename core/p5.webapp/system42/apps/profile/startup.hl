
/*
 * Active Event that creates a GnuPG PGP keypair. Will
 * execute on a different thread
 */

create-event:sys42.create-pgp-keypair
  eval-x:x:/+/0/*
  fork
    p5.crypto.create-pgp-keypair
      identity:x:/../*/_identity?value
      password:x:/../*/_password?value
      seed:x:/../*/_seed?value
