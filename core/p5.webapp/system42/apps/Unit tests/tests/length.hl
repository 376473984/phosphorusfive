/*
 * Contains unit tests for testing [p5.string.length] in system
 */

/*
 * Tests that [p5.string.length] behaves as expected when given a constant
 */
constant
  p5.string.length:Thomas Hansen
  if:x:/-?value
    !=:int:13
    throw:@"Assert error, [p5.string.length] misbehaved!"

/*
 * Tests that [p5.string.length] behaves as expected when given an expression
 */
expression
  _data:Thomas Hansen
  p5.string.length:x:/-?value
  if:x:/-?value
    !=:int:13
    throw:@"Assert error, [p5.string.length] misbehaved!"
