/*
 * Contains unit tests for testing generic string events in system.
 */

/*
 * Test that trimming a string works.
 */
p5.string.trim
  _data:@" foo bar "
  p5.string.trim:x:/-?value
  if:x:/-?value
    !=:foo bar
    throw:@"Assert error, [p5.string.trim] misbehaved!"


/*
 * Test that [p5.string.to-upper] behaves.
 */
p5.string.to-upper
  _data:@"foo bar"
  p5.string.to-upper:x:/-?value
  if:x:/-?value
    !=:FOO BAR
    throw:@"Assert error, [p5.string.to-upper] misbehaved!"


/*
 * Test that [p5.string.to-lower] behaves.
 */
p5.string.to-lower
  _data:@"Foo Bar"
  p5.string.to-lower:x:/-?value
  if:x:/-?value
    !=:foo bar
    throw:@"Assert error, [p5.string.to-lower] misbehaved!"

