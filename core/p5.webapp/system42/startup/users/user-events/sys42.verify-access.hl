
/*
 * Checks to see if the user has access to the given page object
 */

set-event:sys42.verify-access
  set:x:/..?value
    src:bool:true
  if:x:/../*/_arg
    sys42.get-user
    if:x:/-/*/role?value
      not-equals:admin
      and:x:/./-/*/role?value
        not-equals:x:/../*/_arg?value
      set:x:/..?value
        src:bool:false


